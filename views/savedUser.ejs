<!DOCTYPE html>
<html>
  <head>
    <link href="/assets/styles.css" rel="stylesheet" type="text/css" />
    <script src="libraryNav.js"></script>
    <script src="user.js"></script>
      <title>Saved Student Info</title>
  </head>
  <body>
    <%- include('partials/nav') %>

    <h1>Student Database</h1>
    <form action="/collection" method="POST">
        <table id="list" border="1"align="center">
           <tr>
               <th>User Id</th>
               <th>First Name </th>
               <th>Last Name</th>
               <th>Email</th>
               <th>Address1</th>
               <th>Address2</th>
               <th>City</th>
               <th>State</th>
               <th>Zip Code</th>
               <th>County</th>
             </tr>

          <% for (var i =0; i< data.length; i++){ %>
            <tr>
            <td> <%= data[i]._id %> </td>
            <td> <%= data[i].firstName %> </td>
            <td> <%= data[i].lastName %> </td>
            <td> <%= data[i].email %> </td>
            <td> <%= data[i].address1 %> </td>
            <td> <%= data[i].address2 %> </td>
            <td> <%= data[i].city %> </td>
            <td> <%= data[i].zipCode %> </td>
            <td> <%= data[i].county %> </td>
            <td><a href="/update/<%= data[i]._id %>" class="update">Update</a></td>
            <td><button type="submit" name="idToDel" value="<%= data[i]._id %>">Delete</button></td>
            <td> </td>
          </tr>
            <%}%>



       </table>
     </form>
       <span id="val"</span>

  </body>
</html>
